{
  "name": "@hivellm/nexus-sdk",
  "version": "0.12.0",
  "description": "Official TypeScript/JavaScript SDK for Nexus Graph Database",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "publish:wsl": "export BROWSER=wslview && npm publish"
  },
  "keywords": [
    "nexus",
    "graph",
    "database",
    "cypher",
    "neo4j",
    "graph-database",
    "typescript",
    "sdk"
  ],
  "author": "HiveLLM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hivellm/nexus.git",
    "directory": "sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/hivellm/nexus/issues"
  },
  "homepage": "https://github.com/hivellm/nexus/tree/main/sdks/typescript#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

