{
  "name": "@hivellm/n8n-nodes-nexus",
  "version": "0.12.0",
  "description": "n8n community node for Nexus Graph Database - Execute Cypher queries and graph operations",
  "keywords": [
    "n8n-community-node-package",
    "nexus",
    "graph",
    "database",
    "cypher",
    "neo4j",
    "graph-database"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hivellm/nexus",
  "author": {
    "name": "HiveLLM",
    "email": "info@hivellm.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hivellm/nexus.git",
    "directory": "sdks/n8n"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/nodes/Nexus/Nexus.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NexusApi.credentials.js",
      "dist/credentials/NexusUser.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Nexus/Nexus.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
